<div class="row">
	<div class="large-12 columns">

		<div class="section-container auto" data-section>
			<section class="active">
				<p class="title" data-section-title><a href="#panel1">Personal information</a></p>
				<div class="content" data-section-content>
					
						<h6 class="subheader"> <strong>Type of client: </strong> <%=@client.client_type.name %> </h6>
						<h6 class="subheader">  <strong>Name: </strong> <%=@client.name %> </h6>
						<h6 class="subheader">  <strong>Person in charge: </strong> <%=@client.person_in_charge %> </h6>
						<h6 class="subheader">  <strong>Email: </strong> <%=@client.email %> </h6>						
						<h6 class="subheader"> <strong>Address: </strong> <%=@client.address %> </h6>
						<h6 class="subheader">  <strong>NIF: </strong> <%=@client.nif %>  </h6>
						<h6 class="subheader">  <strong>Location: </strong> <%=@client.location %> </h6>
						<h6 class="subheader">  <strong>Postal code: </strong> <%=@client.postal_code %> </h6>
						<h6 class="subheader">  <strong>Mobile phone: </strong> <%=@client.mobile_phone %> </h6>
						<h6 class="subheader">  <strong>Phone: </strong> <%=@client.phone %> </h6>

				</div>
			</section>
			<section>
				<p class="title" data-section-title><a href="#panel2">Section 2</a></p>
				<div class="content" data-section-content>
					<p>Content of section 2.</p>
				</div>
			</section>
		</div>
	</div>
</div>